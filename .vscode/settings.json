{
    "files.exclude": {
        "**/.git/": true,
    },
    "search.exclude": {
        ".vscode/": true,
        "**/assets/": true,
        "**/dist/": true,
        "**/gulp/": true,
        "**/vendor/": true,
        "**/.*": true,
        "**/gulpfile.js": true,
        "**/*.txt": true,
        "**/*.json": true,
        "**/*.lock": true,
        "**/*.xml": true,
        "**/*.css": true,
        "**/*.yaml": true,
        "**/*.yml": true,
        "**/*.pot": true,
    },
    "stylelint.validate": [
        "css",
        "less",
        "postcss",
        "scss",
        "sass",
    ],
    "stylelint.snippet": [
        "css",
        "less",
        "postcss",
        "scss",
        "sass",
    ],
    "phpsab.snifferEnable": true,
}
